{
  "repos": [
    {
      "name": "fas-sanity",
      "path": "../..",
      "role": "sanity"
    },
    {
      "name": "fas-cms",
      "path": "../../../fas-cms-fresh",
      "role": "cms"
    }
  ],
  "envKeyFamilies": [
    "SANITY_PROJECT_ID",
    "NEXT_PUBLIC_SANITY_PROJECT_ID",
    "SANITY_DATASET",
    "NEXT_PUBLIC_SANITY_DATASET",
    "SANITY_READ_TOKEN",
    "SANITY_TOKEN"
  ],
  "envFiles": [
    ".env",
    ".env.local",
    ".env.development",
    ".env.production",
    ".env.development.local"
  ],
  "maxDocsPerType": 200
}
