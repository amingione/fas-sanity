{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Sanity Studio in Chrome",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3333",
      "webRoot": "${workspaceFolder}",
      "runtimeExecutable": "/Applications/Google Chrome.app/Contents/MacOS/Google Chrome"
    },
    {
      "name": "Sanity Dev",
      "type": "pwa-node",
      "request": "launch",
      "runtimeExecutable": "~/.nvm/versions/node/v20.19.0/bin/node",
      "program": "${workspaceFolder}/node_modules/sanity/bin/sanity",
      "args": [
        "dev"
      ],
      "cwd": "${workspaceFolder}"
    }
  ]
}
